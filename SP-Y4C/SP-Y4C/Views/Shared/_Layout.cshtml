<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <environment include="Development">
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>
        <link href="~/lib/bootstrap/dist/css/bootstrap.css" rel="stylesheet" />
        <link href="https://cdn.datatables.net/1.10.15/css/dataTables.bootstrap.min.css" rel="stylesheet" />
        <link href="https://cdn.datatables.net/responsive/2.1.1/css/responsive.bootstrap.min.css" rel="stylesheet" />
        <link href="https://cdn.datatables.net/buttons/1.6.1/css/buttons.dataTables.min.css" rel="stylesheet" />
        @*<link href="https://cdn.datatables.net/responsive/2.2.3/css/responsive.dataTables.min.css" rel="stylesheet" />*@

    </environment>
</head>
<body>
    <nav class="navbar navbar-inverse navbar-static-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li><a asp-area="" asp-controller="Home" asp-action="Index">Home</a></li>
                    <li><a asp-area="" asp-controller="Engagement" asp-action="Index">Engagement</a></li>
                    <li><a asp-area="" asp-controller="AdminConsole" asp-action="Index">Admin</a></li>
                    <li><a asp-area="" asp-controller="Reports" asp-action="Index">Reports</a></li>
                    <li><a asp-area="" asp-controller="Questions" asp-action="Index">Question Management</a></li>
                    <li><a asp-area="" asp-controller="Questions" asp-action="Archived">Archived Questions</a></li>
                </ul>
            </div>
        </div>
    </nav>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; 2019 - Yoga 4 Change. All Rights Reserved.</p>
        </footer>
    </div>

    <environment include="Development">
        <script src="https://cdn.datatables.net/1.10.20/css/jquery.dataTables.min.css"></script>
        <script src="https://cdn.datatables.net/1.10.20/js/jquery.dataTables.min.js"></script>
        <script src="https://cdn.datatables.net/1.10.15/js/dataTables.bootstrap4.min.js "></script>
        <script src="https://cdn.datatables.net/buttons/1.6.1/js/dataTables.buttons.min.js "></script>
        <script src="https://cdn.datatables.net/buttons/1.6.1/js/buttons.html5.min.js "></script>
        <script src="https://cdn.datatables.net/buttons/1.6.1/js/buttons.print.min.js "></script>
        <script src="~/js/jszip.js"></script>
    </environment>

    @RenderSection("Scripts", required: false)
</body>
</html>